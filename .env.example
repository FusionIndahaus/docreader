# Основные настройки сервера
SERVER_PORT=8080
MAX_FILE_SIZE_MB=50
MAX_RESPONSES=20

# N8N интеграция
N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/your-webhook-id

# Пути к файлам
STATIC_DIR=static
ONEC_CONFIG_PATH=config/onec.json

# 1С интеграция (базовые настройки)
ONEC_ENABLED=true
ONEC_AUTO_SEND=false
ONEC_BASE_URL=http://your-1c-server:port/your-base
ONEC_USERNAME=your_username
ONEC_PASSWORD=your_password
ONEC_TIMEOUT=30

# Логирование
LOG_LEVEL=INFO

# Окружение
GO_ENV=development